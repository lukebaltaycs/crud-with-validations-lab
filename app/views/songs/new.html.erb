<h2> New Song Form</h2>
<%= form_for @song do |f| %>
    <label>Title: </label>
    <%= f.text_field :title%><br>
    <label>Has this song been release? </label>
    # <%= f.select :released, options_for_select([['Yes', true], ['No', false]])%><br>
    <%= f.check_box :released %>
    <label>Release Year: </label>
    <%= f.text_field :release_year %>
    # <%= f.select :release_year, options_for_select(@song.do_thing) %><br>
    <label>Artist Name: </label>
    <%= f.text_field :artist_name %><br>
    <label>Genre: </label>
    <%= f.text_field :genre %><br>
    <%= f.submit %>

<% end %>